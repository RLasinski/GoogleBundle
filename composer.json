{
    "name": "antimattr/google-bundle",
    "description": "The GoogleBundle adds the ability to add various google-related services to your application. These include Google Analytics, Adwords and Static Maps.",
    "keywords": ["google", "analytics", "adwords", "maps"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "antimattr",
            "homepage": "https://github.com/antimattr"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": "~2.3",
        "symfony/assetic-bundle": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "AntiMattr\\GoogleBundle": ""
        }
    },
    "target-dir" : "AntiMattr/GoogleBundle"
}
